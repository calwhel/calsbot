modules = ["python-3.11", "postgresql-16", "nodejs-20"]
[agent]
expertMode = true
integrations = ["python_openai_ai_integrations:1.0.0"]

[nix]
channel = "stable-25_05"
packages = ["glibcLocales", "libxcrypt"]

[workflows]

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 39041
externalPort = 3000

[deployment]
deploymentTarget = "vm"
run = ["bash", "start.sh"]
